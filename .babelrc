{
  "env": {
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    }
  },
  "presets": [
    [
      "@babel/preset-env",
      {
        //"modules": false, // Parcel flips this to 'false', but we need it true (default) for node
        "useBuiltIns": false,
        "shippedProposals": true
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties", 
    "react-hot-loader/babel"
  ]
}