{
  "plugins": ["transform-class-properties", "lodash"],
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "useBuiltIns": true
    }]
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "production": {
      "presets": [ ["minify", { "mangle": false }] ],
      "plugins": ["transform-react-remove-prop-types", "lodash"]
    }
  }
}
