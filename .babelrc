{
    "presets": [
        "@babel/preset-react",
        [
            "@babel/preset-env", {
                "useBuiltIns": "entry",
                "targets": {
                    "node": "current", 
                    "edge": "16",
                    "firefox": "59",
                    "safari": "11", 
                    "ios": "11"
                }
            }
        ]
    ],
    "plugins": ["@babel/plugin-proposal-class-properties"]
}